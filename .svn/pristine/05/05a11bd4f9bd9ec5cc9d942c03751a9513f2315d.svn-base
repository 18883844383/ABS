.circle {
	width: 90px;
	height: 90px;
	position: absolute;
	border-radius: 50%;
	background: #F46853;
	right:20px;top:12px;overflow:hidden;
}
.pie_left, .pie_right {
	width:90px; 
	height:90px;
	position: absolute;
	top: 0px;left: 0px;
}
.left, .right {
	width:90px; 
	height:90px;
	background:#00A393;
	border-radius: 50%;
	position: absolute;
	top: 0px;left: 0px;
}
