package com.cqut.service;

import java.util.List;

import com.cqut.bean.User;
/**
 * 用户service
 * @author hshuai
 *
 */
public interface IUserService {
	/**
	 * 用户登录方法
	 * @param user
	 * @return 
	 * 默认false为登录失败
	 */
	
	User login(User user);  
	/**
	 * 用户注册  
	 * @param user
	 * @return
	 * 默认false为注册成功
	 */
